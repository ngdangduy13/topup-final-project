{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ANDROID_HOME=${HOME}/Library/Android/sdk react-native run-android --variant debug",
    "androide": "yarn emulator & yarn android",
    "android:release": "ANDROID_HOME=${HOME}/Library/Android/sdk cd android && ./gradlew assembleRelease --stacktrace && cd ..",
    "android:debug": "adb shell input keyevent 82",
    "android:release-test": "ANDROID_HOME=${HOME}/Library/Android/sdk react-native run-android --variant release",
    "emulator": "${HOME}/Library/Android/sdk/tools/emulator -avd Nexus-5X-API-24",
    "ios": "react-native run-ios --simulator=\"iPhone 7\"",
    "ios:release": "react-native run-ios --configuration Release",
    "lint": "tslint -p .",
    "start": "node node_modules/react-native/local-cli/cli.js start --config ../../../../rn-cli.config.js",
    "test": "jest",
    "updateIcon": "yo rn-toolbox:assets --icon ./assets/images/appIcon.png",
    "codePushIos": "code-push release-react techkids/lumileds-ios ios --deploymentName Production",
    "codePushAndroid": "code-push release-react techkids/lumileds-android android --deploymentName Production"
  },
